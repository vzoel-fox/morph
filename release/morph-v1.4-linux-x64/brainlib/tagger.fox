; ==============================================================================
; TAGGER.FOX - Path Index Registry (like __init__.py)
; ==============================================================================
; This file indexes all paths and assigns IDs to symbols
; Other files use "Ambil ID" to import specific symbols

; Path imports (lowercase ambil)
ambil "corelib/lib/string.fox"
ambil "corelib/lib/vector.fox"
ambil "corelib/lib/hashmap.fox"
ambil "corelib/lib/math.fox"
ambil "corelib/lib/bitwise.fox"
ambil "corelib/lib/fixed_point.fox"
ambil "corelib/lib/module.fox"
ambil "corelib/lib/import.fox"

