{
  "name": "Elsa",
  "scopeName": "text.html.elsa",
  "fileTypes": ["elsa"],
  "patterns": [
    {
      "name": "meta.frontmatter.elsa",
      "begin": "^---\\s*$",
      "end": "^---\\s*$",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.frontmatter.elsa" }
      },
      "endCaptures": {
        "0": { "name": "punctuation.definition.frontmatter.elsa" }
      },
      "patterns": [
        {
          "name": "meta.frontmatter.yaml.elsa",
          "include": "source.yaml"
        }
      ]
    },
    {
      "name": "markup.fenced_code.block.morph.elsa",
      "begin": "^(\\s*)(```)(morph:live|morph:graphics|morph:interactive|morph:chart|morph:system)\\s*$",
      "end": "^(\\1)(```)",
      "beginCaptures": {
        "2": { "name": "punctuation.definition.markdown.begin.elsa" },
        "3": { "name": "entity.name.function.elsa" }
      },
      "endCaptures": {
        "2": { "name": "punctuation.definition.markdown.end.elsa" }
      },
      "patterns": [
        {
          "include": "source.morph"
        }
      ]
    },
    {
      "name": "markup.fenced_code.block.morph.elsa",
      "begin": "^(\\s*)(```)(morph)\\s*$",
      "end": "^(\\1)(```)",
      "beginCaptures": {
        "2": { "name": "punctuation.definition.markdown.begin.elsa" },
        "3": { "name": "entity.name.function.elsa" }
      },
      "endCaptures": {
        "2": { "name": "punctuation.definition.markdown.end.elsa" }
      },
      "patterns": [
        {
          "include": "source.morph"
        }
      ]
    },
    {
      "name": "markup.interpolation.elsa",
      "match": "\\{([a-zA-Z_][a-zA-Z0-9_]*)\\}",
      "captures": {
        "1": { "name": "variable.other.elsa" }
      }
    },
    {
      "name": "markup.heading.1.elsa",
      "match": "^(#{1})\\s+(.*)$",
      "captures": {
        "1": { "name": "punctuation.definition.heading.elsa" },
        "2": { "name": "entity.name.section.elsa" }
      }
    },
    {
      "name": "markup.heading.2.elsa",
      "match": "^(#{2})\\s+(.*)$",
      "captures": {
        "1": { "name": "punctuation.definition.heading.elsa" },
        "2": { "name": "entity.name.section.elsa" }
      }
    },
    {
      "name": "markup.heading.3.elsa",
      "match": "^(#{3})\\s+(.*)$",
      "captures": {
        "1": { "name": "punctuation.definition.heading.elsa" },
        "2": { "name": "entity.name.section.elsa" }
      }
    },
    {
      "name": "markup.bold.elsa",
      "match": "(\\*\\*)(.*?)(\\*\\*)",
      "captures": {
        "1": { "name": "punctuation.definition.bold.begin.elsa" },
        "2": { "name": "markup.bold.elsa" },
        "3": { "name": "punctuation.definition.bold.end.elsa" }
      }
    },
    {
      "name": "markup.italic.elsa",
      "match": "(\\*)(.*?)(\\*)",
      "captures": {
        "1": { "name": "punctuation.definition.italic.begin.elsa" },
        "2": { "name": "markup.italic.elsa" },
        "3": { "name": "punctuation.definition.italic.end.elsa" }
      }
    },
    {
      "name": "markup.underline.link.elsa",
      "match": "\\[([^\\]]+)\\]\\(([^\\)]+)\\)",
      "captures": {
        "1": { "name": "string.other.link.title.elsa" },
        "2": { "name": "markup.underline.link.elsa" }
      }
    },
    {
      "name": "markup.raw.inline.elsa",
      "match": "(`)(.*?)(`)",
      "captures": {
        "1": { "name": "punctuation.definition.raw.begin.elsa" },
        "2": { "name": "markup.raw.inline.elsa" },
        "3": { "name": "punctuation.definition.raw.end.elsa" }
      }
    }
  ]
}
